  	</dependency>
  </dependencies>
  <dependencyManagement>
  	<dependencies>
  		<dependency>
  			<groupId>org.deeplearning4j</groupId>
  			<artifactId>dl4j-examples</artifactId>
  			<version>0.9.1</version>
  		</dependency>
  	</dependencies>
  </dependencyManagement>
</project>